    "start": "node dist/index.js",
    "pm2": "pm2 start --name 'social-oembed' './dist/index.js'"
  },
  "keywords": [
    "fastify",
    "oembed",
    "social",
    "api",
    "embed",
    "media",
    "social network"
  ],
  "dependencies": {
    "@fastify/autoload": "^5.4.1",
    "@fastify/env": "^4.1.0",
    "@fastify/sensible": "^5.1.1",
    "fastify": "^4.9.2",
    "fastify-cli": "^5.5.1",
    "fastify-plugin": "^4.3.0",
    "rimraf": "^3.0.2",
    "tsconfig-paths": "^4.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.29.2",
    "@sinclair/typebox": "^0.25.2",
    "@types/node": "^18.11.8",
    "eslint": "^8.26.0",
    "nodemon": "^2.0.20",
    "tap": "^16.3.0",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.8.4"
  }
