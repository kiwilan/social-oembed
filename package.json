    "release:patch": "standard-version --no-verify --release-as patch"
  },
  "dependencies": {
    "@fastify/autoload": "5.4.1",
    "@fastify/cors": "^8.1.1",
    "@fastify/env": "^4.1.0",
    "@fastify/sensible": "5.1.1",
    "@fastify/swagger": "8.1.0",
    "@sinclair/typebox": "^0.25.2",
    "cheerio": "1.0.0-rc.12",
    "fastify": "4.9.2",
    "fastify-plugin": "4.3.0",
    "node-fetch": "^3.2.10",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.29.3",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@commitlint/cz-commitlint": "17.1.2",
    "@types/node": "18.11.8",
    "commitizen": "4.2.5",
    "esbuild": "0.15.12",
    "esbuild-plugin-pino": "1.2.5",
    "eslint": "^8.26.0",
    "husky": "8.0.1",
    "inquirer": "9.1.4",
    "json-schema-to-ts": "2.5.5",
    "lint-staged": "13.0.3",
    "pino": "8.7.0",
